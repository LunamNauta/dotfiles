{
    "workbench.colorTheme": "Catppuccin Mocha",
    "editor.fontFamily": "'CaskaydiaCove Nerd Font Mono', 'monospace', monospace",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "editor.fontLigatures": true,
    "explorer.confirmDelete": false,
    "catppuccin-noctis-icons.hidesExplorerArrows": false,
    "workbench.iconTheme": "catppuccin noctis icons",
    "terminal.integrated.initialHint": false,
    "find-it-faster.general.batTheme": "Catppuccin Mocha",
    "find-it-faster.general.openFileInPreviewEditor": true,
    "find-it-faster.general.restoreFocusTerminal": true,
    "find-it-faster.general.useTerminalInEditor": true,
    "find-it-faster.general.killTerminalAfterUse": true,
    "editor.detectIndentation": false,
    "explorer.confirmDragAndDrop": false,
    "typescript.updateImportsOnFileMove.enabled": "always"
}